import java.util.Scanner;


public class MinInt {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		String str = "";
		
		for(int i = 0; i < n-1; i++){
			if(i==0){
			str = str() + " int)";
			}else{
				str = str() + " " + str + ")";
			}
		}
		System.out.println(str);
	}
	
	static String str(){
		return "min(int,";
	}
}

